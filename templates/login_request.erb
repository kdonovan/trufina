    <Name>
      <First>John</First>
      <Surname>Doe</Surname>
    </Name>
    <DateOfBirth>1964-09-01</DateOfBirth>
    <ResidenceAddress timeframe="current">
      <StreetAddress>1 Main Street</StreetAddress>
      <StreetAddress>Suite E</StreetAddress>
      <City>Burlingame</City>
      <State>CA</State>
      <PostalCode>94010</PostalCode>
