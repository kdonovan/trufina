<?xml version="1.0" encoding="UTF-8"?>
<TrufinaLoginRequest xmlns="http://www.trufina.com/truapi/1/0">
  <PID><%= @config[:credentials][:PID] %></PID>
  <PRT><%= @prt %></PRT>
  <PAK><%= @config[:credentials][:PAK] %></PAK>
  <CancelURL><%= @config[:local_endpoints][:cancel] %></CancelURL>
  <SuccessURL><%= @config[:local_endpoints][:success] %></SuccessURL>
  <FailureURL><%= @config[:local_endpoints][:failure] %></FailureURL>
  <AccessRequest>
    <Name>
      <First />
    </Name>
    <Age/>
    <ResidenceAddress>
      <State/>
    </ResidenceAddress>
  </AccessRequest>
